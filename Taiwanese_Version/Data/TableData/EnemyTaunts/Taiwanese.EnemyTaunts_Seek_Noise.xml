<?xml version="1.0" encoding="utf-8"?>
<TAUNTS>
  <TAUNT>
    <uiIndex>0</uiIndex>
    <szText>I heard that!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>1</uiIndex>
    <szText>Who's there?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>2</uiIndex>
    <szText>What was that?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>3</uiIndex>
    <szText>Hey! What the...</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>4</uiIndex>
    <szText>Show yourself!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>5</uiIndex>
    <szText>I warn you... my detection skills are at their PEAK!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>6</uiIndex>
    <szText>What the hell was that?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>7</uiIndex>
    <szText>I'm not deaf, you know?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>8</uiIndex>
    <szText>Hey!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>9</uiIndex>
    <szText>Come out, coward!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>10</uiIndex>
    <szText>Hey! Who's making all that racket?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>11</uiIndex>
    <szText>Just show yourself, wherever you are!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>12</uiIndex>
    <szText>Hey! I've heard something suspicious!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>13</uiIndex>
    <szText>Anybody there?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>14</uiIndex>
    <szText>Shoulda check it out.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>15</uiIndex>
    <szText>Is someone being clever?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>16</uiIndex>
    <szText>Strange noises...</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>17</uiIndex>
    <szText>It was just a wind... wait a sec, there's no wind!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>18</uiIndex>
    <szText>Show yourself! We're friendly... and don't know how to fight.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>19</uiIndex>
    <szText>I got a bad feeling on this one.</szText>
  </TAUNT>
</TAUNTS>
