<?xml version="1.0" encoding="utf-8"?>
<TAUNTS>
  <TAUNT>
    <uiIndex>0</uiIndex>
    <szText>Does it hurt to be dead?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>1</uiIndex>
    <szText>Bingo!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>2</uiIndex>
    <szText>Excellence is not a skill. It's an attitude.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>3</uiIndex>
    <szText>Poor thing! He's bleeding.. Hold on, I'll find you a tampon.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>4</uiIndex>
    <szText>How's karma tasting? Not so good?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>5</uiIndex>
    <szText>Doctor is in!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>6</uiIndex>
    <szText>Operation succesful!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>7</uiIndex>
    <szText>You taste like chicken!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>8</uiIndex>
    <szText>You know what would be nice right now? Fava beans and a glass of chianti!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>9</uiIndex>
    <szText>Chop chop chop!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>10</uiIndex>
    <szText>Fresh meat!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>11</uiIndex>
    <szText>I'll drink your blood!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>12</uiIndex>
    <szText>Don't worry, it's disinfected!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>13</uiIndex>
    <szText>Mmmmmincemeat!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>14</uiIndex>
    <szText>Bullseye!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>15</uiIndex>
    <szText>Watch out, the lightning may strike... Oh, it just did!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>16</uiIndex>
    <szText>I see you... and now I can see through you!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>17</uiIndex>
    <szText>Normally I wouldn't hit a woman... Oh, who am I kidding!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>18</uiIndex>
    <szText>See? Girls can fight too!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>19</uiIndex>
    <szText>Girls power!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>20</uiIndex>
    <szText>I now pronounce you... the man and the bullet.</szText>
  </TAUNT>
</TAUNTS>
