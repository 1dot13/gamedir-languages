	AMMO_MONSTER, 7
	AMMO_KNIFE, 8
	AMMO_HE, 9
	AMMO_HEAT, 10
	AMMO_SLEEP_DART, 11
	AMMO_FLAME, 12 - don't use - may crash game

	
-->
