<?xml version="1.0" encoding="UTF-8"?>
<TAUNTS>
  <TAUNT>
    <uiIndex>0</uiIndex>
    <szText>They're here!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>1</uiIndex>
    <szText>Now the fun can start!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>2</uiIndex>
    <szText>I've waited for this looong time.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>3</uiIndex>
    <szText>Sucker with the lowest body count buys drinks!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>4</uiIndex>
    <szText>They have no idea what they are in for...</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>5</uiIndex>
    <szText>Everyone get ready!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>6</uiIndex>
    <szText>Killing time!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>7</uiIndex>
    <szText>I call dibs on any rocket launchers we loot from corpses!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>8</uiIndex>
    <szText>Finally! Some carnage!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>9</uiIndex>
    <szText>Oh, I can't wait to get my hands on them!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>10</uiIndex>
    <szText>Finally, my training will pay off.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>11</uiIndex>
    <szText>Everybody, wake up!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>12</uiIndex>
    <szText>Time to earn my paycheck...</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>13</uiIndex>
    <szText>Now I'll be able to prove myself.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>14</uiIndex>
    <szText>We have tactical advantage!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>15</uiIndex>
    <szText>Maybe I'll get a medal!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>16</uiIndex>
    <szText>We're under attack!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>17</uiIndex>
    <szText>Oh, ain't that just great?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>18</uiIndex>
    <szText>Can't guy get a nap around here?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>19</uiIndex>
    <szText>Enemy attack. That's something new.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>20</uiIndex>
    <szText>Yay. 'Cause nobody saw that coming.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>21</uiIndex>
    <szText>I'm totally gonna desert as soon as we start losing.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>22</uiIndex>
    <szText>Thanks for turning my shitty day into complete disaster, assholes!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>23</uiIndex>
    <szText>Take positions!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>24</uiIndex>
    <szText>I hoped this would never happen...</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>25</uiIndex>
    <szText>Fire at will!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>26</uiIndex>
    <szText>It was bound to happen eventually.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>27</uiIndex>
    <szText>Alert! Alert!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>28</uiIndex>
    <szText>Intruders!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>29</uiIndex>
    <szText>We've got company!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>30</uiIndex>
    <szText>Enemy detected!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>31</uiIndex>
    <szText>Enemy over here!</szText>
  </TAUNT>
</TAUNTS>