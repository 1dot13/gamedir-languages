<?xml version="1.0" encoding="utf-8"?>
<TAUNTS>
  <TAUNT>
    <uiIndex>0</uiIndex>
    <szText>Fuck, my eyeballs!</szText>
    <szTextCensored>Crap, my eyeballs!</szTextCensored>
  </TAUNT>
  <TAUNT>
    <uiIndex>1</uiIndex>
    <szText>I can't see shit!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>2</uiIndex>
    <szText>Who turned off the lights?!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>3</uiIndex>
    <szText>Oh, my eyes!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>4</uiIndex>
    <szText>Oh God, I'm blind!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>5</uiIndex>
    <szText>Holy shit, priest was right! It DOES make you blind!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>6</uiIndex>
    <szText>Somebody help me! I can't see!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>7</uiIndex>
    <szText>You think I won't get you 'cause I'm blind? I can smell your fear!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>8</uiIndex>
    <szText>My eyes are on fire!</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>9</uiIndex>
    <szText>Hello darkness, my old friend.</szText>
  </TAUNT>
</TAUNTS>
