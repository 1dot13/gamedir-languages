<?xml version="1.0" encoding="utf-8"?>
<TAUNTS>
  <TAUNT>
    <uiIndex>0</uiIndex>
    <szText>Attrape !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>1</uiIndex>
    <szText>Et c'est parti !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>2</uiIndex>
    <szText>Prends-toi ça !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>3</uiIndex>
    <szText>Et un pour toi !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>4</uiIndex>
    <szText>Mouahaha !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>5</uiIndex>
    <szText>Attrape ça sale porc !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>6</uiIndex>
    <szText>Bouffe-moi ça !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>7</uiIndex>
    <szText>Et pourquoi pas un peu d'acupuncture ?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>8</uiIndex>
    <szText>Voici une marque de ma gratitude !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>9</uiIndex>
    <szText>Viens, j'ai une jolie lame qui brille pour toi.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>10</uiIndex>
    <szText>Garde la monnaie !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>11</uiIndex>
    <szText>Aucun retour n'est accepté !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>12</uiIndex>
    <szText>En voilà d'autres qui sortent de leur trou.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>13</uiIndex>
    <szText>Fous-toi ça dans le cul, c'est pour la température !</szText>
    <szTextCensored>Prends ça, c'est pour la température !</szTextCensored>
  </TAUNT>
  <TAUNT>
    <uiIndex>14</uiIndex>
    <szText>Tiens, minable !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>15</uiIndex>
    <szText>Eh mais, c'est Noël ! Et tu as été TRÈS méchant !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>16</uiIndex>
    <szText>Étrangle-toi avec ça !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>17</uiIndex>
    <szText>Les voilà !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>18</uiIndex>
    <szText>Tous aux abris !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>19</uiIndex>
    <szText>Grenade !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>20</uiIndex>
    <szText>Ça va péter !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>21</uiIndex>
    <szText>L'un de nous va se blesser et ça ne sera pas moi !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>22</uiIndex>
    <szText>Ta vie s'achève ici, corne-cul !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>23</uiIndex>
    <szText>Merde, pendant un instant, j'ai cru que j'avais jeté la goupille...</szText>
    <szTextCensored>Bon sang, pendant un instant, j'ai cru que j'avais jeté la goupille...</szTextCensored>
  </TAUNT>
  <TAUNT>
    <uiIndex>24</uiIndex>
    <szText>Tiens ma bière et regarde ça !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>25</uiIndex>
    <szText>Offre spéciale ! Deux pour le prix d'une !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>26</uiIndex>
    <szText>Une livraison spéciale !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>27</uiIndex>
    <szText>Voici un échantillon gratuit de nos produits, monsieur !</szText>
  </TAUNT>
</TAUNTS>
