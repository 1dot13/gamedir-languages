<?xml version="1.0" encoding="utf-8"?>
<TAUNTS>
  <TAUNT>
    <uiIndex>0</uiIndex>
    <szText>T'as entendu !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>1</uiIndex>
    <szText>Qui est là ?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>2</uiIndex>
    <szText>Qu'est-ce que c'était ?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>3</uiIndex>
    <szText>Eh ! C'est quoi ce... ?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>4</uiIndex>
    <szText>Montre-toi !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>5</uiIndex>
    <szText>Je te préviens... Mon attention est à son MAX !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>6</uiIndex>
    <szText>C'est quoi ce merdier ?</szText>
    <szTextCensored>C'est quoi ce binz ?</szTextCensored>
  </TAUNT>
  <TAUNT>
    <uiIndex>7</uiIndex>
    <szText>Tu sais, je ne suis pas sourd ?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>8</uiIndex>
    <szText>Hé !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>9</uiIndex>
    <szText>Sors, lâche !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>10</uiIndex>
    <szText>Eh ! Qui a fait tout ce boucan ?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>11</uiIndex>
    <szText>Où que tu sois, viens te présenter !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>12</uiIndex>
    <szText>Hé ! J'ai entendu un drôle de bruit !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>13</uiIndex>
    <szText>Il y a quelqu'un ?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>14</uiIndex>
    <szText>Je dois aller vérifier.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>15</uiIndex>
    <szText>Quelqu'un jouerait-il au malin par ici ?</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>16</uiIndex>
    <szText>D'étranges bruits...</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>17</uiIndex>
    <szText>C'était juste le vent... Attends une seconde... Il n'y a pas de vent !</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>18</uiIndex>
    <szText>Montre-toi ! On ne te veut aucun mal... Et on est inoffensifs.</szText>
  </TAUNT>
  <TAUNT>
    <uiIndex>19</uiIndex>
    <szText>J'ai un mauvais feeling cette fois.</szText>
  </TAUNT>
</TAUNTS>
